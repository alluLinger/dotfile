{
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "filetypes": ["c", "cpp", "objc", "objcpp", "cc", "c++"],
      "initializationOptions": {
          "cache": {
              "directroy": "/tmp/ccls"
          }
      }
    },
    "rust": {
      "command": "rust-analyzer",
      "filetypes": ["rust"],
      "rootPatterns": ["Cargo.toml"]
    }
  },
  "http.proxy": "127.0.0.1:1081"
}
